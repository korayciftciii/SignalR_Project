
@model List<ResultCategoryDto>
<ul class="filters_menu">
    <li class="active" data-filter="*">All</li>
    @foreach (var item in Model)
    {
        if (item.CategoryStatus)
        {

            <li data-filter=".@item.CategoryName.ToLower()">@item.CategoryName</li>
        }
        continue;
    }
</ul>